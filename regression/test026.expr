fun a(x, y) begin
    res := 0;
    asm (
    "movl  #0,    %ebx",
    "movl  #1,    %ecx",
    "movl  %ebx,  %eax",
    "addl  %ecx,  %eax",
    "movl  %eax,  #2": x, y, res);
    return res
end

fun b(y) begin
  y := 5;
  write(y);
  return y
end

x := 2;
write(b(x));
